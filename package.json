{
  "name": "judgepad",
  "version": "1.0.0",
  "type": "module",
  "description": "A martial arts scoring app.",
  "main": "index.js",
  "repository": "git@github.com:rlaakkol/judgepad.git",
  "scripts": {
    "start": "vite",
    "test": "vitest run --config ./vitest.config.ts",
    "test:watch": "vitest --config ./vitest.config.ts",
    "test:lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:inspect": "eslint-config-inspector",
    "build": "vite build",
    "serve": "vite build && vite preview",
    "format": "prettier --write .",
    "cf:deploy": "pnpm build && wrangler deploy"
  },
  "author": "Riku Lääkkölä",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@cloudflare/vite-plugin": "^1.17.1",
    "@eslint/config-inspector": "^1.4.2",
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.5.0",
    "jquery": "^3.7.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.7.4",
    "redux-mock-store": "^1.5.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^6.4.1",
    "vite-plugin-pwa": "1.0.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.54.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "dompurify": "^3.3.1",
    "i18next": "^25.7.3",
    "lodash": "^4.17.21",
    "marked": "^16.4.2",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.3",
    "react-i18next": "^15.7.4",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.11.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "uuid": "^11.1.0"
  }
